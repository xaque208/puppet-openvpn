{
  "name": "zleslie-openvpn",
  "version": "2.1.1",
  "author": "zleslie",
  "summary": "Puppet powered OpenVPN",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-openvpn.git",
  "project_page": "https://github.com/xaque208/puppet-openvpn",
  "issues_url": "https://github.com/xaque208/puppet-openvpn/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "12",
        "11"
      ]
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.6",
        "6.5",
        "6.4",
        "6.3",
        "6.2",
        "6.1",
        "6.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "9",
        "8"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 8.0.0"
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib","version_requirement":">=4.25.1"} 
  ],
  "data_provider": "hiera"
}
